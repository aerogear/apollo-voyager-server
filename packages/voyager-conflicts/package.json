{
  "name": "@aerogear/voyager-conflicts",
  "version": "0.4.0",
  "description": "A package to provide conflict handler framework for GraphQL server",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc --build tsconfig.json",
    "watch": "tsc --build tsconfig.json --watch",
    "test": "ava '*.test.ts' '**/*.test.ts'",
    "compile:clean": "tsc --build tsconfig.json --clean"
  },
  "author": "AeroGear Team<aerogear@googlegroups.com>",
  "license": "Apache-2.0",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "ava": "1.0.0-rc.2",
    "chai": "^4.1.2",
    "nyc": "^11.7.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
