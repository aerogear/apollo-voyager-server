{
  "name": "@aerogear/voyager-audit",
  "version": "0.5.0-rc2",
  "description": "Adds audit logging capabilities to Apollo Voyager Server.",
  "author": "AeroGear Team<aerogear@googlegroups.com>",
  "homepage": "http://aerogear.org",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aerogear/voyager-server/tree/master/packages/voyager-audit"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "tsc --build tsconfig.json",
    "watch": "tsc --build tsconfig.json --watch",
    "compile:clean": "tsc --build tsconfig.json --clean",
    "test": "ava -v '*.test.ts' '**/*.test.ts'"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aerogear/voyager-tools": "^0.5.0-rc2",
    "graphql-tools": "4.0.4",
    "pino": "5.11.1"
  },
  "devDependencies": {
    "@types/pino": "5.20.0",
    "ava": "1.2.1",
    "ts-node": "8.0.2",
    "typescript": "3.3.3333"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
